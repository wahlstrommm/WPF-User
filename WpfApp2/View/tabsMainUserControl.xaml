<UserControl x:Class="WpfApp2.View.tabsMainUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WpfApp2.View"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <TabControl Height="300" x:Name="TabsController">
            <TabItem Width="50" Height="20" Header="Översikt">
                <local:overViewControler/>
            </TabItem>
            <TabItem Width="50" Height="20" Header="Personal">
                <local:PersonalView/>
            </TabItem>
            <TabItem Width="50" Height="20" Header="Info">
                <local:InfoView/>
            </TabItem>
        </TabControl>
    </Grid>
</UserControl>
